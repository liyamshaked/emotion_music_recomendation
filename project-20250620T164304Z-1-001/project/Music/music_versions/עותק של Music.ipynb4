userId":"08555115476902020877"}}},"execution_count":409,"outputs":[]},{"cell_type":"code","source":["thirst_rows = music_df[music_df['emotion'] == 'thirst']\n","pink_rows = music_df[music_df['emotion'] == 'pink']\n","true_rows = music_df[music_df['emotion'] == 'True']\n","combined_rows = pd.concat([thirst_rows, pink_rows, true_rows])\n","df = music_df.copy()\n","df = df[~df['emotion'].isin(['thirst', 'pink', 'True'])]\n","df.loc[df['emotion'] == 'angry', 'emotion'] = 'anger'\n","df.groupby('emotion').size()\n","df = df.drop_duplicates()\n","top_genres = df['Genre'].value_counts().nlargest(11).index\n","df['Genre']=df['Genre'].apply(lambda x: x if x in top_genres else 'other')\n","df['Genre']=df['Genre'].replace('Unknown,Unknown,Unknown','other')\n","#print(df['Genre'].value_counts(normalize=True).sum())"],"metadata":{"id":"wlr4hJ_Cl2eA","executionInfo":{"status":"ok","timestamp":1746111987988,"user_tz":-180,"elapsed":639,"user":{"displayName":"Tahel Barak","userId":"08555115476902020877"}}},"execution_count":426,"outputs":[]},{"cell_type":"code","source":["from sklearn.preprocessing import StandardScaler\n","from sklearn.decomposition import PCA\n","from sklearn.metrics.pairwise import cosine_similarity\n","import numpy as np\n","import pandas as pd\n","\n","def recommend_similar_songs_pca_weighted(song_name, artist_name, top_n=5, n_components=5):\n","    # Step 1: Define features (only numerical audio features)\n","    feature_cols = [\n","        'Tempo','Loudness','Energy',\n","        'Danceability','Positiveness','Speechiness',\n","        'Liveness','Acousticness','Instrumentalness'\n","    ]\n","    # Step 2: Clean the data\n","    df_clean = df.dropna(subset=feature_cols)\n","    df_clean = df_clean.drop_duplicates(subset=['song', 'artist']).reset_index(drop=True)\n","\n","    # Step 3: Scale features\n","    scaler = StandardScaler()\n","    X_scaled = scaler.fit_transform(df_clean[feature_cols])\n","\n","    # Step 4: PCA to compute feature weights\n","    pca = PCA()\n","    X_pca = pca.fit_transform(X_scaled)\n","    W = pca.components_.T  # (n_features, n_components)\n","\n","    # Step 5: Sum squared loadings over top components\n","    feature_importance = np.sum(W[:, :n_components]**2, axis=1)\n","    weights = feature_importance / np.mean(feature_importance)  # normalize\n","\n","    # Step 6: Apply weights to scaled data\n","    X_weighted = X_scaled * weights\n","\n","    # Step 7: Create song-to-index lookup\n","    song_to_index = pd.Series(df_clean.index, index=list(zip(df_clean['song'], df_clean['artist'])))\n","\n","    lookup_key = (song_name, artist_name)\n","    if lookup_key not in song_to_index:\n","        print(f\"Song '{song_name}' by '{artist_name}' not found. Returning random selection.\")\n","        return df_clean.sample(n=top_n)[['song', 'artist', 'Genre', 'emotion'] + feature_cols]\n","\n","    # Step 8: Compute cosine similarity\n","    idx = song_to_index[lookup_key]\n","    target_vec = X_weighted[idx].reshape(1, -1)\n","    sim_scores = cosine_similarity(target_vec, X_weighted)[0]\n","\n","    # Step 9: Sort and exclude the original song\n","    top_indices = np.argsort(sim_scores)[::-1]\n","    top_indices = top_indices[top_indices != idx][:top_n]\n","\n","    # Step 10: Return top-N similar songs\n","    result = df_clean.iloc[top_indices][['song', 'artist', 'Genre', 'emotion'] + feature_cols].copy()\n","    result['similarity'] = sim_scores[top_indices]\n","\n","    return result\n"],"metadata":{"id":"V5ktv45MVgdl","executionInfo":{"status":"ok","timestamp":1746111496140,"user_tz":-180,"elapsed":3,"user":{"displayName":"Tahel Barak","userId":"08555115476902020877"}}},"execution_count":412,"outputs":[]},{"cell_type":"code","source":["recommendations = recommend_similar_songs_pca_weighted(\"IDGAF\", \"Dua Lipa\")\n","print(recommendations)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"YVP3a77QWwwZ","executionInfo":{"status":"ok","timestamp":1746111564380,"user_